{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": "off",
    "no-console": "off",
    "import/no-extraneous-dependencies": "off",
    "no-underscore-dangle": "off",
    "object-curly-newline": "off",
    "no-unused-vars": "off",
    "camelcase": "off",
    "comma-dangle": "off"
  }
}
